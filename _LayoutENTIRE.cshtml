        $.ajax({
            url: "@Url.Action("Email", "Home")",
            type: "POST",
            data: formData,
            dataType: "json",
            success: function (result) {
                $("#successmsg").html("Email Sent!");
            }


        })
        $("#successmsg").html("Email Sent!");
        $("#EmailForm")[0].reset();

    })


</script>
