<script>
    

    current = '';
    other1 = '';
    other2 = '';
    y = null;
    z = true;
    function extend(x) {


        if (x != 2 && x != 3) {
            current = 'centriq';
            other1 = "midtown";
            other2 = "tubbys";
        }
        if (x != 1 && x != 3) {
            current = 'midtown';
            other1 = 'centriq';
            other2 = 'tubbys';
        }
        if (x != 1 && x != 2) {
            current = 'tubbys';
            other1 = 'centriq';
            other2 = 'midtown';
        }

        //Opens up the click if its the first option
        if (y == null) {
            document.getElementById(current).style.height = "25em";

            document.getElementById(current + "1").style.fontSize = "1.35em";
            $("#" + current + "1").addClass("sizing-1");
            document.getElementById(current + "2").style.webkitTransform = "rotate(180deg)";
            document.getElementById(current + "2").style.transform = "rotate(180deg)";



        }
        //keeps track of the last call to see if it needs to close the current option.
        if (y == x) {
            if (z == false) {

                document.getElementById(current).style.height = "25em";

                document.getElementById(current + "1").style.fontSize = "1.35em";
                $("#" + current + "1").addClass("sizing-1");

                document.getElementById(current + "2").style.webkitTransform = "rotate(180deg)";
                document.getElementById(current + "2").style.transform = "rotate(180deg)";
                z = true;
            }
            else {
                document.getElementById(current).style.height = "7.5em";
                document.getElementById(current + "1").style.fontSize = "0";
                $("#" + current + "1").removeClass("sizing-1");
                document.getElementById(current + "2").style.webkitTransform = "none";
                document.getElementById(current + "2").style.transform = "none";
                z = false;
            }



        }
        //if the last selection was not clicked runs normal functionality
        if (y != x && y != null) {
            document.getElementById(current).style.height = "25em";

            $("#" + current + "1").addClass("sizing-1");
            document.getElementById(current + "1").style.fontSize = "1.35em";

            document.getElementById(current + "2").style.webkitTransform = "rotate(180deg)";
            document.getElementById(current + "2").style.transform = "rotate(180deg)";

        }

        document.getElementById(other1).style.height = "7.5em";
        document.getElementById(other1 + "1").style.fontSize = "0";
        $("#" + other1 + "1").removeClass("sizing-1");
        document.getElementById(other1 + "2").style.webkitTransform = "none";
        document.getElementById(other1 + "2").style.transform = "none";

        document.getElementById(other2).style.height = "7.5em";
        document.getElementById(other2 + "1").style.fontSize = "0";
        $("#" + other2 + "1").removeClass("sizing-1");
        document.getElementById(other2 + "2").style.webkitTransform = "none";
        document.getElementById(other2 + "2").style.transform = "none";
        y = x;


    }

</script>

